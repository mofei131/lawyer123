
.box {
	position: absolute;
	width: 100%;
	height: 100%;
	background: rgb(0, 0, 0, 0.5);
	top: 0;
	z-index: 50;
	display: flex;
	justify-content: center;
	align-items: center;
}
.boxchild {
	padding: 20rpx 0;
	-webkit-animation: mymove 0.5s;
	        animation: mymove 0.5s;
	position: fixed;
	/* top: 400rpx; */
	margin: 0 auto;
	background-color: #FFFFFF;
	height: 300rpx;
	width: 90%;
	border-radius: 10rpx;
	z-index: 10;
	justify-content: center;
	margin: auto;
	/* left: 0; */
}


